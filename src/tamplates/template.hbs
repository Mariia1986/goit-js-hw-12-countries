{{#each this}}

<h2 class="country-name">{{name}}</h2>
<p>Capital {{capital}}</p>
<p>Population {{population}}</p>
<div class="flag">
    <img width="800" src="{{flag}}" alt="{{name}}">
</div>
<p>Languages</p>
<ul>
    {{#each languages}}
    <li>
        {{this.name}}</li>
    {{/each}}
</ul>
{{/each}}